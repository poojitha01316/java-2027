import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new  Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0){
		    int A=sc.nextInt();
		    int B=sc.nextInt();
		    int C=sc.nextInt();
		    int D=sc.nextInt();
		    if(A>B+C+D || B>A+C+D || C>A+B+D || D>A+B+C){
		        System.out.println("YES");
		    }else{
		        System.out.println("NO");
		    }
		    
		}

	}
}
