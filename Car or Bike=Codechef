import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0){
		    int X=sc.nextInt();
		    int Y=sc.nextInt();
		    if(X<=1 && Y<=5){
		        System.out.println("BIKE");
		    }else if(X<=4 && Y<=2){
		        System.out.println("CAR");
		    }else{
		        System.out.println("SAME");
		    }
		}

	}
